#!/bin/bash

colours="ffffff 99eeff 99ffee eeff99 ffee99 ff99ee ff0000 00ff00 0000ff"
modes="cwDark cwBright"

mkdir -p sheets

for cwMode in $modes; do
    colouredweb --"${cwMode}"
    
    for colour in $colours; do
        colouredweb --build="${colour},sheets/${cwMode}-${colour}.css"
    done
done
